pipeline {
    agent any
    stages
    {
        stage('Git Clone')
        {
            steps{
            sh 'rm -rf spring3hibernate  git clone https://github.com/Deveshs23/spring3hibernate.git'
            }
        }
        stage('Code Stability')
        {
            steps{
            sh 'cd spring3hibernate; mvn compile'
            }
        }
    }
    
}
